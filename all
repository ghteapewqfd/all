// combined-scripts.js

// Video.js CSS
var videoJSCSS = document.createElement('link');
videoJSCSS.rel = 'stylesheet';
videoJSCSS.href = 'https://vjs.zencdn.net/7.11.4/video-js.css';
document.head.appendChild(videoJSCSS);

// Video.js JavaScript
var videoJSJS = document.createElement('script');
videoJSJS.src = 'https://vjs.zencdn.net/7.11.4/video.min.js';
document.head.appendChild(videoJSJS);

// Video.js Ads Plugin JavaScript
var videoJSAdsJS = document.createElement('script');
videoJSAdsJS.src = 'https://cdnjs.cloudflare.com/ajax/libs/videojs-contrib-ads/6.6.5/videojs.ads.min.js';
document.head.appendChild(videoJSAdsJS);

// Google IMA SDK JavaScript
var imaSDKJS = document.createElement('script');
imaSDKJS.src = 'https://imasdk.googleapis.com/js/sdkloader/ima3.js';
document.head.appendChild(imaSDKJS);

// Video.js IMA Plugin CSS
var videoJSIMACSS = document.createElement('link');
videoJSIMACSS.rel = 'stylesheet';
videoJSIMACSS.href = 'https://cdnjs.cloudflare.com/ajax/libs/videojs-ima/1.11.0/videojs.ima.min.css';
document.head.appendChild(videoJSIMACSS);

// Video.js IMA Plugin JavaScript
var videoJSIMAJS = document.createElement('script');
videoJSIMAJS.src = 'https://cdnjs.cloudflare.com/ajax/libs/videojs-ima/1.11.0/videojs.ima.min.js';
document.head.appendChild(videoJSIMAJS);
